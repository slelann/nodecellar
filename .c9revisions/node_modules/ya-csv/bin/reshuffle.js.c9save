{"ts":1377525630218,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"var csv = require('../lib/ya-csv'),\n    sys = require('sys');\n\nvar reader = csv.createCsvStreamReader(process.openStdin(), { columnsFromHeader: true });\nvar writer = csv.createCsvStreamWriter(process.stdout);\n\nif (process.argv.length < 3) {\n    sys.error(\"Usage: \" + process.argv[0] + \" \" + process.argv[1] + \" <output columns>\");\n    process.exit(1);\n}\n\nvar outColumns = [];\nfor (var i = 2; i < process.argv.length; i++) {\n    outColumns.push(process.argv[i]);\n}\n\nwriter.writeRecord(outColumns);\nreader.addListener('data', function(data) {\n    var out = [];\n    for (var i = 0; i < outColumns.length; i++) {\n        col = outColumns[i];\n        out[i] = data[col];\n    }\n    writer.writeRecord(out);\n});\n"]],"start1":0,"start2":0,"length1":0,"length2":705}]],"length":705}
